    "@types/lodash.isequal": "^4.5.6",
    "@types/mocha": "^9.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.0.1",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "add": "^2.0.6",
