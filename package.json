    "lodash.isequal": "^4.5.0",
    "mocha": "^10.0.0",
    "moment": "^2.29.4",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "solidity-docgen": "^0.6.0-beta.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.2",
    "bignumber.js": "^9.0.2",
    "dotenv": "^16.0.1",
    "node-cron": "^3.0.0"
  },
  "files": [
    "LICENSE",
    "abis/**",
    "artifacts/**",
    "contracts/**"
  ],
  "packageManager": "yarn@1.22.18",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf ./artifacts ./deployments ./cache ./coverage ./types ./coverage.json && yarn typechain",
    "abi": "hardhat abi",
    "docs": "hardhat docgen",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "cross-env HIDE_DEPLOY_LOG=true HIDE_TASK_LOG=true OPTIMIZER_DISABLED=true hardhat coverage --network hardhat",
    "coverage:file": "cross-env HIDE_DEPLOY_LOG=true HIDE_TASK_LOG=true OPTIMIZER_DISABLED=true hardhat coverage --testfiles",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yml --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yaml,yml}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yml \"**/*.{js,json,md,sol,ts,yaml,yml}\"",
    "remove-logs": "yarn run hardhat remove-logs",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "start-fork": "cross-env FORK_ENABLED=true && yarn compile && hardhat node --network hardhat",
    "console": "hardhat console --network",
    "deploy": "hardhat deploy --write true --network",
    "conf": "hardhat config --network",
    "accounts": "hardhat accounts --network",
    "gems": "hardhat gems --network",
    "deployments": "hardhat deployments --network",
    "get-some-dai": "hardhat get-some-dai --network",
    "get-some-defo": "hardhat get-some-defo --network",
    "get-some-gems": "hardhat get-some-gems  --network",
    "mint-some-gems": "yarn get-some-gems",
    "claim": "hardhat claim --network",
    "permit": "hardhat permit --network",
    "vault": "hardhat vault --network",
    "liquidity": "hardhat liquidity --network",
    "jump-in-time": "hardhat --network localhost jump-in-time",
    "history": "hardhat history --time 7days --network",
    "dev": "cross-env HIDE_SHOW_NETWORK=true yarn get-some-dai localhost && yarn get-some-defo localhost && yarn permit localhost && yarn get-some-gems localhost && yarn jump-in-time --time 8d && yarn claim localhost --id 0 && yarn vault localhost --op stake --id 1 --amount 1 && yarn vault localhost --op stake --id 2 --amount 5 && yarn vault localhost --op unstake --id 2 --amount 4.75 && yarn gems localhost && yarn accounts localhost ",
    "test": "cross-env HIDE_DEPLOY_LOG=true HIDE_TASK_LOG=true OPTIMIZER_DISABLED=true DEBUG='defo:*' hardhat test",
    "gas": "cross-env REPORT_GAS=true HIDE_DEPLOY_LOG=true HIDE_TASK_LOG=true hardhat test",
    "verify": "hardhat etherscan-verify --license GPL-3.0 --solc-input --network"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0"
}
